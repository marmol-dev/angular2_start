{
  "name": "angular2-start",
  "version": "0.0.1",
  "description": "Angular2 start with Typescript, Webpack and Express.",
  "scripts": {
    "postinstall": "./node_modules/typings/dist/bin/typings.js install",
    "watch": "./node_modules/.bin/webpack --watch",
    "build": "./node_modules/.bin/webpack",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/nodemon dist/server/bundle.js",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/bundle.js"
  },
  "license": "Apache2",
  "dependencies": {
    "angular2": "^2.0.0-beta.3",
    "bootstrap": "^3.3.6",
    "css": "2.2.1",
    "es6-shim": "^0.34.2",
    "express": "^4.13.4",
    "lodash": "^3.10.1",
    "parse5": "^1.5.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "typings": "^0.6.8",
    "xhr2": "^0.1.3",
    "zone.js": "^0.5.10"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^1.1.1",
    "nodemon": "^1.8.1",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.3",
    "typings": "^0.6.8",
    "webpack": "^1.12.10",
    "webpack-merge": "^0.7.1"
  }
}
